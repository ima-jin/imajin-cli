{
  "name": "@imajin/cli",
  "version": "0.1.0",
  "description": "LLM-powered universal service interface for dynamic service discovery and orchestration",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "imajin": "./bin/imajin"
  },
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc && npm run fix-imports",
    "watch": "node scripts/watch.js",
    "fix-imports": "node scripts/fix-imports.js",
    "start": "node dist/index.js",
    "cli": "node dist/index.js",
    "cli:dev": "npm run build && npm run cli",
    "cli:help": "npm run cli -- --help",
    "cli:diagnose": "npm run cli diagnose",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "clean": "rimraf dist",
    "clean:all": "rimraf dist node_modules package-lock.json",
    "rebuild": "npm run clean && npm install && npm run build",
    "nuke": "npm run clean:all && npm install && npm run build",
    "git-nuke": "powershell -ExecutionPolicy Bypass -File scripts/nuke-and-rebuild.ps1",
    "sonar": "eslint src/**/*.ts --ext .ts",
    "update-dates": "node scripts/update-task-dates.js",
    "update-dates:tasks": "node scripts/update-task-dates.js docs/prompts/tasks/*.md",
    "update-dates:projects": "node scripts/update-task-dates.js docs/prompts/projects/*.md"
  },
  "keywords": [
    "cli",
    "llm",
    "service-integration",
    "stripe",
    "contentful",
    "automation",
    "fair-protocol"
  ],
  "author": "imajin",
  "license": ".fair LICENSING AGREEMENT",
  "dependencies": {
    "@types/chokidar": "^1.7.5",
    "@types/express": "^5.0.3",
    "@types/js-yaml": "^4.0.9",
    "@types/keytar": "^4.4.0",
    "@types/ws": "^8.18.1",
    "axios": "^1.6.2",
    "axios-cookiejar-support": "^6.0.2",
    "boxen": "^7.1.1",
    "bull": "^4.16.3",
    "chalk": "^5.3.0",
    "cheerio": "^1.1.0",
    "chokidar": "^4.0.3",
    "cli-table3": "^0.6.3",
    "cloudinary": "^2.6.1",
    "commander": "^11.1.0",
    "contentful": "^11.7.3",
    "contentful-management": "^11.54.0",
    "dotenv": "^16.3.1",
    "express": "^5.1.0",
    "figlet": "^1.7.0",
    "glob": "^11.0.2",
    "handlebars": "^4.7.8",
    "inquirer": "^9.2.12",
    "ioredis": "^5.4.1",
    "js-yaml": "^4.1.0",
    "keytar": "^7.9.0",
    "md-to-pdf": "^5.2.4",
    "openapi-types": "^12.1.3",
    "ora": "^7.0.1",
    "prom-client": "^15.1.2",
    "reflect-metadata": "^0.1.13",
    "stripe": "^18.2.1",
    "tough-cookie": "^5.1.2",
    "tsconfig-paths": "^4.2.0",
    "tsyringe": "^4.8.0",
    "uuid": "^11.1.0",
    "winston": "^3.11.0",
    "ws": "^8.18.2",
    "zod": "^3.25.56"
  },
  "devDependencies": {
    "@types/figlet": "^1.5.8",
    "@types/glob": "^8.1.0",
    "@types/handlebars": "^4.0.40",
    "@types/inquirer": "^9.0.7",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.19.1",
    "@types/semver": "^7.7.0",
    "@types/swagger-schema-official": "^2.0.25",
    "@types/triple-beam": "^1.3.5",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.54.0",
    "eslint-plugin-sonarjs": "^3.0.2",
    "jest": "^29.7.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.8.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imajin/imajin-cli.git"
  },
  "bugs": {
    "url": "https://github.com/imajin/imajin-cli/issues"
  },
  "homepage": "https://github.com/imajin/imajin-cli#readme",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
